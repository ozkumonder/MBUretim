    function deleteInCart(id) {
        $.ajax({
            type: "post",
            url: '@Url.Action("DeleteFirms", "ManageFirms")',
            data: { id: id },
            cache: false,
            success: function (data) {
                $("#sepetDiv").html(data);
                GetCart();
            },
            error: function (err) {
                alert(err);
            }
        });
    }
